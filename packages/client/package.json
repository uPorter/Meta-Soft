    "@xrengine/projects": "^1.0.0-rc3",
    "app-root-path": "3.0.0",
    "classnames": "2.3.1",
    "cli": "1.0.1",
    "credential-handler-polyfill": "^2.3.0",
    "cross-env": "7.0.3",
    "express-static-gzip": "^2.1.7",
    "file-regex": "3.1.0",
